    "dotnet.defaultSolution": "SampleApp/SampleApp.sln",
    "dotnetAcquisitionExtension.existingDotnetPath": [
        {
            "extensionId": "ms-dotnettools.csharp",
            "path": "/usr/share/dotnet/shared/Microsoft.NETCore.App"
        }
